{
  "name": "tree-sitter-blom",
  "version": "0.1.0",
  "description": "A programming language built in Go, offering compilation with QBE, direct interpretation, and transpilation to multiple languages",
  "repository": "https://github.com/xhyrom/blom",
  "license": "EUPL-1.2",
  "author": {
    "name": "Jozef Steinh√ºbl",
    "email": "contact@xhyrom.dev",
    "url": "https://xhyrom.dev/"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "blom"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.2.1",
    "node-gyp": "^11.0.0",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.24.6"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "generate": "tree-sitter generate",
    "parse": "tree-sitter parse",
    "highlight": "tree-sitter highlight"
  }
}
